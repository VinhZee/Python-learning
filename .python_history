import turtle
def draw_multicolor_square(t,sz):
    for i in ["red", "puple", "hotpink", "blue"]:
        t.color(i)
        t.forward(sz)
        t.left(90)

wn = turtle.Screen()
tess = turtle.Turtle()
tess.pensize(3)
size = 30
for i in range(15):
    draw_multicolor_square(tess, size)
    size = size + 10
    tess.forward(10)
    tess.right(18)
    
